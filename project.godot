; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="LinMan"
config/description="A Linux Game and Modding Manager."
config/version="0.0.1"
run/main_scene="res://Scenes/Home/home.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="LinMan"
config/features=PackedStringArray("4.3", "Forward Plus")
run/low_processor_mode=true
run/low_processor_mode_sleep_usec=10000
boot_splash/image="res://Images/BootSplash/linman_boot_splash.png"
boot_splash/fullsize=false
config/icon="res://Images/Logo/linman_logo_background.png"
boot_splash/minimum_display_time=5000

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/energy_saving/keep_screen_on=false
window/vsync/vsync_mode=0
